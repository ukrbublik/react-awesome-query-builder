{
  "name": "@react-awesome-query-builder/examples",
  "version": "6.0.0-beta.0",
  "description": "Local hot-reloadable demo for @react-awesome-query-builder",
  "author": "Denis Oblogin <ukrbublik@gmail.com> (https://github.com/ukrbublik)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ukrbublik/react-awesome-query-builder.git",
    "directory": "packages/examples"
  },
  "main": "index.tsx",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve",
    "build": "cross-env NODE_ENV=production CSS=0 webpack",
    "analyze": "cross-env NODE_ENV=production ANALYZE=1 webpack",
    "lint": "yarn eslint && yarn tsc",
    "eslint": "eslint --ext .jsx --ext .js --ext .tsx --ext .ts .",
    "lint-fix": "eslint --ext .jsx --ext .js --ext .tsx --ext .ts --fix .",
    "tsc": "tsc -p . --noEmit",
    "check-hot": "node ./scripts/check-hot.js"
  },
  "dependencies": {
    "@react-awesome-query-builder/antd": "*",
    "@react-awesome-query-builder/bootstrap": "*",
    "@react-awesome-query-builder/core": "*",
    "@react-awesome-query-builder/material": "*",
    "@react-awesome-query-builder/mui": "*",
    "@react-awesome-query-builder/ui": "*",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-select": "^5.2.2",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.3.10",
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.16.13",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.3",
    "@mui/lab": "^5.0.0-alpha.98",
    "@mui/material": "^5.10.4",
    "@mui/system": "^5.10.4",
    "@mui/x-date-pickers": "^5.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "bootstrap": "^5.1.3",
    "reactstrap": "^9.0.0"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "mini-css-extract-plugin": "^2.2.0",
    "moment-locales-webpack-plugin": "^1.2.0",
    "react-refresh": "^0.14.0",
    "webpack-dev-server": "^4.4.0"
  }
}
