{
  "name": "@react-awesome-query-builder/mui",
  "version": "6.4.2",
  "description": "User-friendly query builder for React. MUI 5 widgets",
  "keywords": [
    "query-builder",
    "query",
    "builder",
    "query builder",
    "react",
    "reactjs",
    "ui",
    "material",
    "material-ui",
    "material-design",
    "mui"
  ],
  "homepage": "https://github.com/ukrbublik/react-awesome-query-builder",
  "bugs": "https://github.com/ukrbublik/react-awesome-query-builder/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ukrbublik/react-awesome-query-builder.git",
    "directory": "packages/mui"
  },
  "license": "MIT",
  "author": "Denis Oblogin <ukrbublik@gmail.com> (https://github.com/ukrbublik)",
  "exports": {
    ".": {
      "types": "./modules/index.d.ts",
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    },
    "./package.json": "./package.json",
    "./css/styles.css": "./css/styles.css",
    "./css/compact_styles.css": "./css/compact_styles.css",
    "./css/styles.scss": "./css/styles.scss",
    "./css/compact_styles.scss": "./css/compact_styles.scss"
  },
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "types": "./modules/index.d.ts",
  "scripts": {
    "build": "sh ./scripts/build-npm.sh",
    "eslint": "eslint --ext .jsx --ext .js --ext .tsx --ext .ts  ./modules/",
    "lint": "npm run eslint && npm run tsc",
    "lint-fix": "eslint --ext .jsx --ext .js --ext .tsx --ext .ts --fix  ./modules/",
    "prepare": "npm run build",
    "tsc": "tsc -p . --noEmit"
  },
  "dependencies": {
    "@material-ui/core": "4.12.4",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/system": "4.12.2",
    "@mui/x-date-pickers": "^6.3.1",
    "@react-awesome-query-builder/ui": "workspace:^",
    "lodash": "^4.17.21",
    "material-ui-confirm": "^3.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-modules-commonjs": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/runtime": "^7.22.5",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@types/react": "^17.0.39",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.35.0"
  },
  "peerDependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/x-date-pickers": "^5.0.0-beta.2 || ^6.0.0",
    "react": "^16.8.4 || ^17.0.1 || ^18.0.0",
    "react-dom": "^16.8.4 || ^17.0.1 || ^18.0.0"
  }
}
