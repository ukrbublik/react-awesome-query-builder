    "@mui/system": "^6.3.1",
    "@mui/x-date-pickers": "^7.23.6",
    "@react-awesome-query-builder/antd": "workspace:^",
    "@react-awesome-query-builder/mui": "workspace:^",
    "antd": "^5.24.5",
