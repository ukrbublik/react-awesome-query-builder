    "@types/lodash": "^4.14.170",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "sass": "^1.77.2",
    "typescript": "~5.4.5",
    "vite": "^5.4.19"
