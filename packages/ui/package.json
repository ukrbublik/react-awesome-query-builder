{
  "name": "@react-awesome-query-builder/ui",
  "version": "6.0.0-beta.4",
  "description": "User-friendly query builder for React. Core React UI",
  "repository": {
    "type": "git",
    "url": "https://github.com/ukrbublik/react-awesome-query-builder.git",
    "directory": "packages/ui"
  },
  "license": "MIT",
  "author": "Denis Oblogin <ukrbublik@gmail.com> (https://github.com/ukrbublik)",
  "exports": {
    ".": {
      "types": "./modules/index.d.ts",
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    },
    "./package.json": "./package.json",
    "./css/styles.css": "./css/styles.css",
    "./css/compact_styles.css": "./css/compact_styles.css",
    "./css/styles.scss": "./css/styles.scss",
    "./css/compact_styles.scss": "./css/compact_styles.scss"
  },
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "types": "./modules/index.d.ts",
  "scripts": {
    "build": "sh ./scripts/build-npm.sh",
    "eslint": "eslint --ext .jsx --ext .js --ext .tsx --ext .ts  ./modules/",
    "lint": "npm run eslint && npm run tsc",
    "lint-fix": "eslint --ext .jsx --ext .js --ext .tsx --ext .ts --fix  ./modules/",
    "prepare": "npm run build",
    "tsc": "tsc -p . --noEmit"
  },
  "dependencies": {
    "@react-awesome-query-builder/core": "workspace:^",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "react-redux": "^7.2.2",
    "redux": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "@babel/plugin-proposal-private-property-in-object": "^7.14.5",
    "@babel/plugin-transform-modules-commonjs": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/runtime": "^7.14.5",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.18",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.35.0"
  },
  "peerDependencies": {
    "react": "^16.8.4 || ^17.0.1 || ^18.0.0",
    "react-dom": "^16.8.4 || ^17.0.1 || ^18.0.0"
  }
}
