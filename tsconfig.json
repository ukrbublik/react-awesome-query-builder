{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "es2017"
    ],
    "esModuleInterop": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "allowJs": true,
    "outDir": "./ts_out",
    "skipLibCheck": true,
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "strictNullChecks": false,
    "noImplicitAny": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "include": ["modules"],
  "exclude": ["node_modules"]
}
