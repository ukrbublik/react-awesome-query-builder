{
  "name": "@react-awesome-query-builder/root",
  "version": "6.0.0-beta.0",
  "private": true,
  "description": "User-friendly query builder for React. Demo: https://ukrbublik.github.io/react-awesome-query-builder",
  "keywords": [
    "query-builder",
    "query",
    "builder",
    "query builder",
    "react-awesome-query-builder",
    "react-query-builder",
    "where",
    "clause",
    "operators",
    "expression",
    "react",
    "reactjs",
    "component",
    "react-component",
    "ui",
    "antd",
    "antdesign",
    "ant-design",
    "material",
    "material-ui",
    "material-design",
    "configurable"
  ],
  "homepage": "https://github.com/ukrbublik/react-awesome-query-builder",
  "bugs": {
    "url": "https://github.com/ukrbublik/react-awesome-query-builder/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ukrbublik/react-awesome-query-builder.git"
  },
  "funding": "https://opencollective.com/react-awesome-query-builder",
  "license": "MIT",
  "author": "Denis Oblogin <ukrbublik@gmail.com> (https://github.com/ukrbublik)",
  "workspaces": [
    "packages/core",
    "packages/ui",
    "packages/antd",
    "packages/mui",
    "packages/material",
    "packages/bootstrap",
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "build-examples": "npm run build --workspace=@react-awesome-query-builder/examples",
    "check-hot": "npm run check-hot --workspace=@react-awesome-query-builder/examples",
    "clean": "sh ./scripts/clean.sh",
    "examples": "npm run start --workspace=@react-awesome-query-builder/examples",
    "for-gpr": "sh ./scripts/for-gpr.sh",
    "for-npm": "sh ./scripts/for-npm.sh",
    "lint": "npm run lint --workspaces",
    "lint-fix": "npm run lint-fix --workspaces",
    "reinstall": "npm run clean && npm i",
    "resmoke": "npm run reinstall && npm run smoke",
    "sandbox": "npm run sandbox-ts",
    "sandbox-js": "npm run start --workspace=@react-awesome-query-builder/sandbox-simple",
    "sandbox-ts": "npm run start --workspace=@react-awesome-query-builder/sandbox",
    "smoke": "npm run lint && npm run test && npm run build-all && npm run check-hot",
    "start": "npm run examples",
    "test": "npm run test --workspace=@react-awesome-query-builder/tests"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.5",
    "@babel/eslint-parser": "^7.14.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "@babel/plugin-proposal-private-property-in-object": "^7.14.5",
    "@babel/plugin-transform-modules-commonjs": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "^7.24.0",
    "lerna": "^6.1.0",
    "process": "^0.11.10",
    "typescript": "~4.4.4"
  },
  "engines": {
    "node": ">=14.15.0",
    "npm": ">=7",
    "yarn": "^1.22.10"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "overrides": {
    "@types/react": "^17"
  }
}
