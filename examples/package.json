{
  "name": "react-awesome-query-builder-demo-local",
  "version": "0.4.0",
  "description": "Local hot-reloadable demo for react-awesome-query-builder",
  "main": "index.tsx",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js --hot --color --display-error-details --no-info",
    "build": "NODE_ENV=production webpack --config webpack.config.js"
  },
  "author": "Denis Oblogin <ukrbublik@gmail.com> (https://github.com/ukrbublik)",
  "dependencies": {
    "antd": ">=4.0.0 <4.5.0",
    "@material-ui/core": "4.11.0",
    "react-awesome-query-builder": "file:..",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.2"
  }
}
